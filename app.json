{
  "expo": {
    "name": "Qwantu",
    "slug": "qwantu",
    "version": "2.2.4",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "application",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.wegah_studios.qwantu"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/adaptive-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      [
        "expo-sqlite",
        {
          "enableFTS": true,
          "useSQLCipher": true
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Urbanist-Regular.ttf",
            "./assets/fonts/Urbanist-Medium.ttf",
            "./assets/fonts/Urbanist-Bold.ttf"
          ],
          "android": {
            "fonts": [
              {
                "fontFamily": "Urbanist",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Urbanist-Regular.ttf",
                    "weight": 400
                  },
                  {
                    "path": "./assets/fonts/Urbanist-Medium.ttf",
                    "weight": 500
                  },
                  {
                    "path": "./assets/fonts/Urbanist-Bold.ttf",
                    "weight": 700
                  }
                ]
              }
            ]
          },
          "ios": {
            "fonts": [
              "./assets/fonts/Urbanist-Regular.ttf",
              "./assets/fonts/Urbanist-Medium.ttf",
              "./assets/fonts/Urbanist-Bold.ttf"
            ]
          }
        }
      ],
      [
        "./plugins/withAndroidDrawables",
        {
          "drawableFiles": [
            "./assets/images/select.xml",
            "./assets/images/share.xml"
          ]
        }
      ],
      "expo-mail-composer",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  }
}
